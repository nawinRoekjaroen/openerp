<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="test_module_form" model="ir.ui.view">
            <field name="name">test.module.form</field>
            <field name="model">test.module</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Name Test">
                    <group colspan="4" col="4">
                        <field name="title"/>
                        <field name="fname" select="1"/>
                        <field name="lname" select="1"/>
                        <field name="age"/>
                        <field name="gender"/>
                        <field name="gender2"/>
                        <field name="dateofbirth"/>
                    </group>
                </form>
            </field> 
        </record>
        <record id="test_module_tree" model="ir.ui.view">
            <field name="name">test.module.tree</field>
            <field name="model">test.module</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Name Test">
                    <field name="title"/>
                    <field name="fname" select="1"/>
                    <field name="lname" select="1"/>
                    <field name="age"/>
                    <field name="gender"/>
                    <field name="gender2"/>
                    <field name="dateofbirth"/>
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_test_seq">
            <field name="name">Test Module</field>
            <field name="res_model">test.module</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="main_menu_test" name="Test Name"/>
	<!-- 
        <manuitem id="menu_test" parent="main_menu_test"
                  name="Information" action="action_test_seq"/>
	-->
    </data>
</openerp>
